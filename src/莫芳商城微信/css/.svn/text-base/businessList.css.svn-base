body{
	background-color: #eeeeee;
}
.banner{
 width: 100%; position: relative;overflow-x: auto; overflow-y:hidden;
}
.swiper-slide img{ width: 100%;border:1px solid #d9d9d9;}
.swiper-slide{text-algin:center; display:-webkit-box; display:-ms-flexbox; display:-webkit-flex; display:flex; -webkit-box-pack:center; -ms-flex-pack:center;
-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center; position:relative; background-size:100%;}
.swiper-slide span{ position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); font-size:14px; color:#ccc; display:block;
width:84px;height:34px;line-height:34px; text-decoration:none; text-align:center;}
.active{ background:url(../images/bussinessList/hbg.png) center center no-repeat;}
.bListBox{
	width: 100%;float: left; margin-bottom:10px;
}
.business-box{width: 100%; float:left; padding-bottom: 15px; background: #fff;}
.business-box2{width: 100%; float:left; padding-bottom: 15px; background: #fff;}
.business-list{position: relative;}
.business-list-img img{display: block; width: 100%;}

.business-list-opacity{background:rgba(0,0,0,0.7);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#b2000000,endColorstr=#b2000000);zoom:1;width: 100%; color: #fff;position: absolute;bottom: 0;left: 0;padding: 9px 20% 11px 14px;font-size: 12px;box-sizing: border-box;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
:root .business-list-opacity{filter:none\9;}/*for IE9*/
.business-list-opacity i{display: inline-block;width: 16px;height: 16px; margin-right: 5px;position: relative;top: 4px;background: url(../images/ss-sprites.png) no-repeat;background-position:-124px -48px; background-size: 225px 225px;}
.praise{width: 44px;height: 43px; background-position: -69px -165px;position: absolute;bottom: 20px;right: 14px;}
.praise.cur{ background-position: -121px -165px;}
.business-list-con{background: #fff;padding: 0 10px 0;position: relative;}
.business-list-praise-num{margin-top: 15px;}
.business-list-title{padding:10px 14% 5px 0;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size: 16px;}
.business-list-con .m{position: absolute;right: 12px;top: 14px;font-size: 13px;color: #8d8d8d;}
.business-list-con .m:before{content: "";position: absolute;top:0;left: -16px;width: 14px;height: 17px;background: url(../images/businessList/map_icon.png) no-repeat;background-size: 100%; display: none;}
.b01{border: 1px solid #f49c0b; color: #f49c0b;}
.b02{border: 1px solid #39a0e5; color: #39a0e5;}
.b03{border: 1px solid #f2746e; color: #f2746e;}
.b04{border: 1px solid #b56ef2; color: #b56ef2;}
.b05{border: 1px solid #39a0e5; color: #39a0e5;}
.business-list-text{overflow: hidden;}
.business-list-text li{border-radius: 4px; float: left;font-size: 12px;padding: 2px 6px; margin: 5px 8px 8px 0;}
.business-list-text li:last-child{margin-right: 0;}
.business-list-praise-num{width: 100%;overflow: hidden; margin-bottom:10px;}
.business-list-praise-num li{position:relative; width:11.11%;display: block; float: left; }
.business-list-praise-num li img{ width: 90%;border-radius: 100%; float:left; margin-left:5%;display: block;}
.business-list-praise-num li span{position: relative;}
.business-list-praise-num li em{font-style: normal; height:100%; width:100%; display: -moz-box;-moz-box-pack: center;-moz-box-align: center;display: -webkit-box;-webkit-box-pack: center;-webkit-box-align: center;display: box;box-pack:center;box-align:center;border-radius: 100%; color: #8d8d8d; text-align: center; font-size: 12px;position: absolute;top: 50%;left: 50%;-webkit-transform: translate(-50%,-50%);-ms-transform: translate(-50%,-50%);transform: translate(-50%,-50%);}
.header{height: 40px;}
.header >*{float: left;}
.header .return{height: 100%;position: relative;width: 40px;}
.header .return a{width:12px;height:34px;background: url(../images/businessList/icon-return.jpg) no-repeat center center;background-size:100%;position: absolute;left: 50%;top: 50%;-webkit-transform: translate(-50%, -50%);-ms-transform: translate(-50%, -50%);transform: translate(-50%, -50%);}
.header-title .return{width:10px;height:34px;background:url(../images/businessList/return_icon.png) no-repeat center center;background-size:100%;position: absolute;left: 16px;top: 50%;-webkit-transform: translate(-50%, -50%);-ms-transform: translate(-50%, -50%);transform: translate(-50%, -50%);}
.header-title .share{width: 20px;height:18px;background:url(../images/businessList/share_icon.png) no-repeat;position: absolute;right: 14px;top: 50%;transform: translate(0,-50%);-webkit-transform: translate(0, -50%);-ms-transform: translate(0, -50%);background-size: 100%;}
.business-details{position: relative; padding-bottom: 0;}
.business-details .praise{top: -23px;z-index: 10;}
.business-list-address,.business-list-iphone,.business-list-notice{border-top: 1px solid #eeeeee; padding: 12px 35px;position: relative;}
.business-list-notice{background: #fff;padding: 12px 0;float: left; width: 100%;}
.business-list-notice-text{padding: 12px 12px;border-top: 1px solid #eeeeee;font-size: 13px;text-align: justify;word-break: break-all;}
.business-list-address .adr,.business-list-iphone .tel,.business-list-notice .notice{position: relative;font-size: 14px;width: 80%; text-align: justify;line-height: 16px;}
.business-list-notice .notice{padding: 0 0 12px 35px;}
.business-list-address .adr{width: 100%;}
.business-list-address .adr:before,.business-list-notice .notice:before,.business-list-iphone .tel:before{content: "";position: absolute;top: 0;left: -20px;width: 16px;height: 16px;background:url(../images/ss-sprites.png) no-repeat;background-size:300px 300px;}
.business-list-address .adr:before{height: 21px; background-position: -128px -44px;}
.business-list-iphone .tel:before{height: 21px;background-position: -145px -44px;}
.business-list-notice .notice:before{height: 21px;width: 19px;background-position: -111px -44px;left: 15px;}
.business-list-address .mi,.business-list-iphone .mi{position: absolute;top:50%;right: 14px; color: #8d8d8d;font-size: 13px;width:10px; height:14px; -webkit-transform:translate(0,-50%); transform:translate(0,-50%);}

.business-image-text{margin-top: 10px;float: left; width: 100%; word-break:break-all;}
.business-image-text p,span{ word-break:break-all;}
.businessAddDiv{
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */
	-webkit-box-sizing:border-box;
	line-height: 20px;text-align: center;background: #fff; margin: 0;font-size: 16px; padding: 12px;border-bottom: thin solid #eeeeee;font-weight: bold;
}
.business-image-text .title{line-height: 20px;position: relative;padding-left: 7px;padding-top: 11px;font-family: 微软雅黑;font-size: 15px;}
.business-image-text .present{padding: 0 12px;background: #fff;margin-bottom: 15px; padding-bottom: 1px;font-size: 13px;overflow: hidden;}
.business-image-text .present:last-child{margin-bottom:0;}
.business-image-text .present .title:before{content: ""; position: absolute;top: 12px;left: 0; height: 16px;width: 2px;background: #8d8d8d;}
.business-image-text .image-text{font-size: 13px;padding: 12px 2px 20px;text-align: justify;}


.shopBanner{ clear:both; width:100%;}
.swiper-slide img{ width: 100%;  border:1px solid #d9d9d9;}
.swiper-container-horizontal>.swiper-pagination-bullets{bottom: 5px;}
.swiper-pagination-bullet{background: #fff;opacity: 1;width: 5px;height: 5px;}
.swiper-pagination-bullet-active{background: #f18e1d;}	
.banner .swiper-container{margin: 10px 0;}
