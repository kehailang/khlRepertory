.bottomShopBox{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	position: fixed; bottom: 0px; width: 100%; height: 45px; border-top: thin solid #eeeeee; z-index:900; max-width: 768px;
}

.bottomShopButton1,.bottomShopButton2{
	float: left; width: 50%; background-color: white; height: 100%; position: relative;
}

.bottomShopButton1:active,.bottomShopButton2:active{
	background-color: #E2E0E0;
}

.bottomShopButton1 div,.bottomShopButton2 div{
	width: 110px;  position: absolute; top:0; left: 50%; padding:8px 0px;
	transform: translate(-50%,0);
	-ms-transform: translate(-50%,0);
	-webkit-transform: translate(-50%,0);
	-o-transform: translate(-50%,0);
	-moz-transform: translate(-50%,0);	
}

.bottomShopButton2{background: #ef490d;color: #fff;}
.bottomShopButton1 div span,.bottomShopButton2 div span{
	float:left;
    margin: 5px;
	width:20px; height:18px;
	background:url(../images/ss-sprites.png) no-repeat;background-size:225px 225px;background-position:-44px -22px;
}

.bottomShopButton1 div p,.bottomShopButton2 div p{
	float:left;  font-size: 14px; line-height: 18px; margin: 0 0 0 0; padding: 5px 0;
}


.bottomShopButton2 div{
	width: 100px;
}


.bottomShopButton2 div span{background-position:2px -21px;margin: 6px 5px;}

.oback{
	width: 100%; height: 100%; position: fixed; background-color: black;  top:0px; left:0px;
	filter:alpha(opacity=50);-moz-opacity: 0.5;opacity: 0.5;z-index: 901; display: none; 
}

.shoppingCart{
	width: 100%; position: fixed; 
	z-index: 902; bottom: 0px;  background-color: #ffffff;display: none; 
}

.shoppingCart_t1{
	width: 100%; float: left; height: 100px; border-bottom: thin solid #eeeeee; position: relative;
}

.shoppingCartt1_Img{
	width: 68px; height: 63px; position: absolute; top:18.5px; left: 10px; border: thin solid #eeeeee; border-radius:2px;
}

.shoppingCart_colseB{
	position: absolute; width: 12px; height: 12px; top:10px; right: 10px;cursor:pointer;
}

.shoppingCartt1_data{
	width: 100%; padding-left: 93px; padding-right: 10px;
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
}

.shoppingCartt1_p1,.shoppingCartt1_p2{
	margin-top:28px;
	width: 100%;text-overflow: ellipsis;/* IE/Safari */-ms-text-overflow: ellipsis;-o-text-overflow: ellipsis; overflow: hidden; white-space:nowrap;-moz-binding: url("ellipsis.xml#ellipsis");
	font-size: 14px; line-height: 16px; 
}

.shoppingCartt1_p2{
	margin-top:6px;
	font-size: 19px; line-height: 21px;  color:#ee4910;
}

.shoppingCart_t2{
	width: 100%; float: left; border-bottom: thin solid #eeeeee; position: relative;
}

.shoppingPo_p,.shoppingPo_p2{
	width:100%; font-size: 14px; padding: 0px 10px; float: left; margin-top:10px;
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; 
}

.shoppingPo_p2{
	padding-right:10px;  margin-bottom:12px; 
}

.shoppingPo_data{
	width:auto;
	padding: 3px 6px; font-size: 12px; color:#cacaca;
	border: thin solid #cacaca; border-radius:4px;
	float: left; margin-left: 10px; margin-top:10px; cursor:pointer;
}
.shoppingPo_data:nth-of-type(1){margin-left: 0;}
.shoppingPo_p3{
	width:100%; font-size: 14px; padding: 0px 10px; float: left; margin-top:10px; margin-bottom:15px;
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; 
}

.addButton,.minusButton{
	width: 20px;height: 20px; border-radius:0px 5px 5px 0px; cursor:pointer; border: thin solid #cacaca; color: #ee4910; float: left; font-size: 18px;line-height: 20px;
}

.addButton img,.minusButton img{
	width: 14px; height: 14px;margin: 3px; float: left; display: block;
}

.minusButton{height: 20px; border-radius:5px 0px 0px 5px;}


.pnumsD{
	width: 30px; height: 20px; border-bottom: thin solid #cacaca; border-top: thin solid #cacaca; float: left;
}

.pnumsD input{
	float:left;
	outline:none;-webkit-appearance: none;
	width: 30px;   margin:0 0 0 0px; padding:0 0 0 0; font-size:12px; border:none; line-height:20px; text-align:center;
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; 
}

.pnumsD input::-webkit-inner-spin-button {
     -webkit-appearance: none;
}
.pnumsD input::-webkit-outer-spin-button {
     -webkit-appearance: none;	/* 有无看不出差别 */
}

.okButton{
	border-top: thin solid #eeeeee;
