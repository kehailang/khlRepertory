/*详情**/
.pd_titleBox{
	width: 100%; float: left; background-color: white; height: 40px;
}

.pd_titleDivA,.pd_titleDivB{
	width: 50%; float: left; height: 40px; 
	box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;
	border-bottom: 4px solid #fff;
	text-align: center; font-size: 16px; font-family: 微软雅黑; vertical-align: middle; line-height: 40px;
}

.pdc_box{
	width: 100%;float: left;
}

.pd_backLine{
	width: 100%; height: 10px; background-color: #eeeeee; float: left;
}

.pdc_title{
	width: 100%;height: 40px; text-align: center; line-height: 40px; font-size: 16px; font-family: 微软雅黑; float: left;
}

.qbComment{
	float: left;
    width: 100%;
    display: block;
    list-style-type: none;
    -webkit-margin-before: 0;
    -webkit-margin-after: 0;
    -webkit-margin-start: 0;
    -webkit-margin-end: 0;
    -webkit-padding-start: 0;
	float: left; width: 100%;
}
.qbComment li{
	padding: 30px;border-bottom: 1px solid #eeeeee;
}
.qbComment li:last-child{
	border-bottom: none;
}
.qbComment .left{
	float: left; width: 45px;height: 45px;overflow: hidden;border-radius: 50%;position:relative;top: -13px;left: 0;
}
.qbComment .left img{
	float: left; width: 45px;height: 45px;overflow: hidden;;
}


.qbComment .right{
	overflow: hidden;
	padding-left: 14px; 
}
.qbComment .right p{
	font-size: 13px;color: #8d8d8d;text-align: justify;word-break: break-all;
}

.pdc_buttonBox{
	width: 100%;float: left; text-align: center; margin-top: 0px;margin-bottom: 20px;
}

.pdc_button{
	width: 60%; background-color: #e2e2e2; -webkit-appearance: none; border-radius:5px; height: 40px;  font-size:15px;font-family: 微软雅黑;
	border:none;
	box-shadow:none;
	-moz-box-shadow:none;
	-webkit-box-shadow:none;
}

.pdc_button img{
	width: 16px; height: 15px; vertical-align: -1px;
}

.pdc_button:ACTIVE {
	background-color: #CCCACA;
}

.pdc_box2{
	width: 100%;float: left; background-color: #eeeeee;
}

.pdc_title2{
	width: 100%;height: 40px; text-align: center; line-height: 40px; font-size: 16px; font-family: 微软雅黑; float: left; background-color: #fff;
}

.pd_img{
	width: 100%; display: block;
}

.parameter{
	overflow: hidden;padding-top: 10px;padding-bottom: 10px;
}
.parameter:nth-child(odd){
	background: #eee;
}
.parameter div:first-child{
	width: 64px;
	line-height: 24px;
	font-size: 15px;
	padding-left: 30px;
	float: left;
	word-break:break-all;
}
.parameter div:last-child{
	color: #8d8d8d;	
	line-height: 24px;
	font-size: 15px;
	padding-left: 30px;
	float: left;
	word-break:break-all;
}

