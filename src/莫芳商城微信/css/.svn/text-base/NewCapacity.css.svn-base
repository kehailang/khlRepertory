@charset "utf-8";

.tNewCapacityBack{
	position: fixed; z-index: 998; width: 100%; height: 100%; top:0; left:0;
	 filter:alpha(opacity=20);    
     -moz-opacity: 0.2;    
     opacity: 0.2;
     background-color:black;
     display:none; 
}

.tNewCapacityAlert{
	box-sizing:border-box;-moz-box-sizing:border-box; /* Firefox */-webkit-box-sizing:border-box; 
	transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%); 	-moz-transform:translate(-50%,-50%); 	-webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%); 
	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius:2px; border:thin solid #eeeeee;
	position: fixed; z-index: 999; background-color: #fff; top:50%; left: 50%;
	box-shadow: 2px 2px 5px #777; padding: 15px 15px;
	overflow: hidden; max-width: 300px; min-width:200px;
	display: none; 
}

.tNewCapacityAlert_text{
	word-break:break-all;float: left;  text-align: center; font-size: 13px;font-family: '微软雅黑'; color: black; width: 100%;display: none; 
}


.tNewCapacityAlert_button{
	transform:translate(-50%,0);-ms-transform:translate(-50%,0); -moz-transform:translate(-50%,0); -webkit-transform:translate(-50%,0); -o-transform:translate(-50%,0); 
	-moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius:5px;
	float: left; width: 100px; color: #fff; font-size: 14px;font-family: '微软雅黑'; background-color: #ee4910; margin-top: 15px; margin-left:50%;
	outline: none; -webkit-appearance: none; border: none;line-height: 16px; padding-bottom: 5px; padding-top: 5px; text-align: center;
	display: none; 
}

.tNewCapacityAlert_button:ACTIVE{
	background-color: #D24312;
}

.tNewCapacityConfirm{
	box-sizing:border-box;-moz-box-sizing:border-box; /* Firefox */-webkit-box-sizing:border-box; 
	transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%); 	-moz-transform:translate(-50%,-50%); 	-webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%); 
	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius:2px; border:thin solid #eeeeee;
	position: fixed; z-index: 999; background-color: #fff; top:50%; left: 50%;
	box-shadow: 2px 2px 5px #777; padding: 15px 15px;
	overflow: hidden; max-width: 320px; min-width:250px; 
	display:none;
}

.tNewCapacityConfirm_text{
	word-break:break-all;float: left;  text-align: center; font-size: 13px;font-family: '微软雅黑'; color: black; width: 100%;
	display: none; 
}

.tNewCapacityConfirmControlBox{
	float: left; width: 100%; position: relative; margin-top: 15px;
	display: none; 
}


.tNewCapacityConfirm_button1{
	-moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius:5px;
	float: left; width: 45%; color: #fff; font-size: 14px;font-family: '微软雅黑'; background-color: #ee4910;line-height:16px; padding-top:5px; padding-bottom:5px;
	outline: none; -webkit-appearance: none; border: none;text-align: center;
	display: none; 
}

.tNewCapacityConfirm_button1:ACTIVE{
	background-color: #D24312;
}

.tNewCapacityConfirm_button2{
	-moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius:5px;
	float: left; width: 45%; color: #fff; font-size: 14px;font-family: '微软雅黑'; background-color: #ee4910; line-height:16px; padding-top:5px; padding-bottom:5px;
	outline: none; -webkit-appearance: none; border: none; margin-left: 10%; text-align: center;
	display: none; 
}

.tNewCapacityConfirm_button2:ACTIVE{
	background-color: #D24312;
}
