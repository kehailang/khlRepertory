*{margin:0;padding:0}html,body{font:14px "微软雅黑"}
.songshu{margin:10px 15px 0; margin-top: 20px;}
.songshu p{ margin-top: 5px;}
.songshu:last-child{margin-bottom: 20px;}
.aO_title{width:100%; height:50px;background-color:#FFFFFF; border-bottom-color:#eeeeee; border-bottom-style:solid; border-bottom-width:thin; position: relative;}
.aO_titleI{width:26px; height:26px; margin-top:14px; float:left; margin-left:10px;}
.aO_titleDD{font-size:16px; float:left; font-weight:bold; letter-spacing:2px; color:#323232; position: absolute; top:15px; left: 50%; -webkit-transform: translate(-50%,0%);-ms-transform: translate(-50%,0%);transform: translate(-50%,0%);}
.titleCon{position: absolute;right: 14px;top: 17px;color: #8d8d8d; font-size: 12px;}

.toastBack{
	-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;
	transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%);
	position: fixed; width: 200px; height: 50px; background-color: #000000; top:50%; left: 50%;z-index: 800;display: none;
	filter:alpha(opacity=90);-moz-opacity: 0.9;opacity: 0.9; 
}
.toastText{
	transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%);
	position: fixed;z-index: 801;color:#fff;position: fixed;top:50%; left: 50%;font-size: 14px;display: none;
}