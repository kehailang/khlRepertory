@charset "utf-8";
body{
	padding: 0; margin: 0; font-family: "微软雅黑"; background-color: #ee4911; position: relative;
}

img{display: block;}

input[type=number] {  
    -moz-appearance:textfield;  
}  
input[type=number]::-webkit-inner-spin-button,  
input[type=number]::-webkit-outer-spin-button {  
    -webkit-appearance: none;  
    margin: 0;  
}  

.bigBox{
	float: left; position: relative;width: 100%;
}

.bgImg{
	width: 100%;
}

.textBox{
	transform:translate(-50%,0);-ms-transform:translate(-50%,0);-moz-transform:translate(-50%,0);-webkit-transform:translate(-50%,0); -o-transform:translate(-50%,0);	
	box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;	
	-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;
	width: 86%; position: absolute; height: 285px; background-color: #ffffff;border: 2px solid #000000;
	top: 47%; left: 50%;
}

.Div1{width:100%; float:left;}

.Div1T1{width: 100%; font-size: 20px; color: black; text-align: center; float: left; margin-top: 20px;}
.Div1T1 span{color:#ff0000;}
.Div1T2{width: 100%; font-size: 12px; color: black; text-align: center; float: left; margin-top: 5px;}
