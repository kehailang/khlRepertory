/* 图片自适应 */
img {
	width: 100%;
max-width: 100%;
height: auto;
width:auto\9; /* ie8 */
-ms-interpolation-mode:bicubic;/*为了照顾ie图片缩放失真*/
    display: block;
}
a{text-decoration:none; color:#666666; font-family:Microsoft YaHei,Tahoma,Arial,sans-serif;}
button,input{color:#666; outline: none; resize: none;-webkit-appearance: none;border: none;font-family: 'Microsoft YaHei';}
html{max-width: 768px;margin: 0 auto;} 
body{font-family: 'Microsoft YaHei';font-size:14px;max-width: 768px;margin: 0 auto;}
/*优惠码*/
.bigBBox{position: relative;}
.shareYl {position:absolute;top:50%;left:50%;width:370px;height:auto;overflow:hidden;}
.shareYl > div {margin-top: 240px;}
.exchange{width: 100%;height: 45%;position: absolute;bottom: 0;left: 0;box-shadow: 0px -20px 60px #000;padding: 0 8%;box-sizing: border-box; background:rgba(0,0,0,0.85);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#7f000000);zoom:1;}
:root .exchange{filter:none\9;}/*for IE9*/
.exchange-group{width: 100%;height: 38px;margin-bottom: 15px;position: relative;}
.exchange-source,.exchange-code,.exchange-iphone,.exchange-security{height: 100%;border-radius: 4px;}
.exchange-group input{width: 100%;height: 100%;border-radius: 4px;padding: 0 10px;box-sizing: border-box;color: #000;font-size: 15px;}
.exchange-source input{background: #fff url(../../images/public/triangle.png) no-repeat 97% center;
    background-size: 12px 6px;}
.exchange-choose{width: 100%;position: absolute;top: 38px;left: 0;z-index: 100;box-shadow: 0px 0px 6px #575555;display: none;}   
.exchange-choose span{width: 100%;height: 30px;display: inline-block;background: #fff;padding: 0 10px;box-sizing: border-box;text-align: left;}
.exchange-btn{height: 38px;display: table;width: 100%;margin-top: 20px;}
.exchange-btn a{width: 100%;height: 100%;display: inline-block;color: #fff;text-align: center; background: #ff3f26;box-shadow: 0px 4px 0px #cf2d18;display: table-cell;vertical-align: middle;border-radius: 4px;font-size: 16px;}
.hot-line{position: static;bottom: 12px;left: 0;width: 100%;text-align: center;color:#504e4e;font-size: 12px;}
.hot-line a{color: #1784c7;}
.exchange-conBg{ padding:5%;box-sizing:border-box;border-radius: 5px;background:rgba(0,0,0,0.5);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#7f000000);zoom:1;}
:root .exchange-conBg{filter:none\9;}/*for IE9*/
.exchange-bg .exchange-btn{margin-bottom: 4px;}
.exchange-security .yanzheng{width: 60%;float: left;}
.exchange-security .verify{width: 40%;height: 100%; float: left;border-radius: 6px;background: #ffac2d;padding: 0;color: #fff;box-sizing: border-box;    border: 3px solid #fff;}
.exchange-security{background: #fff;}
.exchange-security input{background: none;}

footer p{color: #504e4e;font-size: 12px;margin: 0;padding:0 1px;box-sizing: border-box;text-align: center;}
footer{width: 100%;position: absolute;bottom: 6px;}
.exchange-conBg{text-align: center;}
.content-bg{background: white; position: relative; width: 90%;margin: 0 auto;}
.content-bg h3{font-size: 20px;color: #333333;border-bottom: 1px dashed #dfdfdf;height: 44px;line-height: 44px;margin-bottom: 0;text-align: center;}
.content-bg:before{content: "";position: absolute;left: -2px;top: 0;width: 2px;height: 100%;background: url(../images/left-bg.png) no-repeat;background-size: cover;}
.content-bg:after{content: "";position: absolute;right: -2px;top: 0;width: 2px;height: 100%;background: url(../images/right-bg.png) no-repeat;background-size: cover;}
.content-box{padding: 7px 0;overflow: hidden;}
.get-money{float: left; width: 25%;border-right: 1px dashed #dfdfdf;font-size: 12px; padding: 0 5px; text-align: center;}
.get-money em{font-size: 27px;color: #f18e1d;display: block;font-style: normal;}
.in-account{overflow: auto;height: 52px; position: relative;font-size: 14px; text-align: left;padding: 8px 10px 0;box-sizing: border-box;}
.in-account em{font-size: 14px;color: #f18e1d;font-style: normal;}
.money-bg{position: absolute;right: 0;bottom: 0;width: 40px;height: 40px;background: url(../images/money-bg.png) no-repeat;background-size: cover;}



.tNewCapacityBack{
	position: fixed; z-index: 998; width: 100%; height: 100%; 
	 filter:alpha(opacity=50);    
     -moz-opacity: 0.5;    
     opacity: 0.5;
     display:none; 
}

.tNewCapacityAlert{
	box-sizing:border-box;-moz-box-sizing:border-box; /* Firefox */-webkit-box-sizing:border-box; 
	transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%); 	-moz-transform:translate(-50%,-50%); 	-webkit-transform:translate(-50%,-50%); -o-transform:translate(-50%,-50%); 
	-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius:2px; border:thin solid #eeeeee;
	position: fixed; z-index: 999; background-color: #fff; top:50%; left: 50%;
	box-shadow: 2px 2px 5px #777; padding: 15px 15px;
	overflow: hidden; max-width: 300px; min-width:200px;
	display: none; 
	
}

.tNewCapacityAlert_text{
	word-break:break-all;float: left;  text-align: center; font-size: 13px;font-family: '微软雅黑'; color: black; width: 100%;
}


.tNewCapacityAlert_button{
	transform:translate(-50%,0);-ms-transform:translate(-50%,0); -moz-transform:translate(-50%,0); -webkit-transform:translate(-50%,0); -o-transform:translate(-50%,0); 
	-moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius:5px;
	float: left; width: 100px; color: #fff; font-size: 14px;font-family: '微软雅黑'; background-color: #ff3f26; margin-top: 15px; margin-left:50%;
	outline: none; -webkit-appearance: none; border: none;line-height: 16px; padding-bottom: 5px; padding-top: 5px; text-align: center;
}