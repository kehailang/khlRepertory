body{background-color: #eeeeee;}
/***********个人中心css***************/
.personalHeader{width: 100%;overflow: hidden;position: relative;}
.personalHeader img{width: 100%;}
.personalHeader .blurBg{margin-top: -5px;padding-bottom: 65%;}
.blur{position: absolute;top: 0;left: 0;
		filter: url(blur.svg#blur); /* FireFox, Chrome, Opera */
-webkit-filter: blur(5px); /* Chrome, Opera */
   -moz-filter: blur(5px);
    -ms-filter: blur(5px);    
        filter: blur(5px);	
		filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=10, MakeShadow=false); /* IE6~IE9 */
	}
.personalPos{position:absolute;left: 0;bottom: 0;width: 100%;height: 88%;}
.botBox{background:rgba(0,0,0,0.3); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#7f000000);zoom:1; width: 100%;padding:1.1% 0 1.6%; position: absolute;left: 0;bottom: 0;}
:root .botBox{filter:none\9;}/*for IE9*/
.botBox li{width: 100%; float: left; text-align: center;} 
.botBox li span{display: block; color: #f3edd9;}
.botBox li span:first-child{color:#86c159 ; font-size: 24px; height: 31px;}
.botBox li span:last-child{font-size: 13px;margin-top: -3px; height: 18px;}
.botBox li:last-child{box-sizing: border-box;position: relative;}
.borderBg{position: absolute;top: 26%;left:50%;transform: translate(-50%,-50%);-webkit-transform: translate(-50%, -50%);-ms-transform: translate(-50%, -50%);width: 24.5%; padding-bottom: 24.5%;border-radius:50%;background:rgba(0,0,0,0.3);filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7f000000,endColorstr=#7f000000);zoom:1;}
.personalPos p{height: 48%;width: 100%;text-align: center;font-size: 14px;color: #fff; position: absolute;left: 0;bottom: 0;}
.order,.orderPadding{margin-bottom: 10px;}
.order a{width:100%; color: #000;font-size: 14px;display:inline-block;}
.order li{border-bottom: 1px solid #e5e5e5; background: #fff; padding: 12px 15px 12px 41px;position: relative;}
.order li:last-child{border-bottom: none;}
.order .iconMyorder,.order .iocnSignin,.order .iconShare,.order .iconPreferentia,.order .iconCollection,.order .iconFootprint,.order .iconAddress,.order .iocnHelp,.order .iconService,.order .iconOpinion,.iconLiked,.iconLook,.iocnLine,.iconHotLine,.iconIdea{ width: 24px;height: 24px;background-size: 100%;left: 0;position: absolute; top:50%;-webkit-transform: translate(0,-50%);transform: translate(0,-50%);background: url(../images/ss-sprites.png) no-repeat; background-size: 225px 225px;}
.order .iconMyorder{left: 10px;background-position:-52px -130px;}
.order .iocnSignin{background-position:-127px -103px;}
.order .iconShare{background-position:-25px -130px;}
.order .iconPreferentia{background-position:-50px -103px;}
.order .iconAddress{background-position:-76px -130px;}
.order .iconService{background: url(../images/my/icon-gr-kf.png) no-repeat;background-size: 100% 100%;}
.iconLiked{background: url(../images/my/icon-gr-zg.png) no-repeat;background-size: 100% 100%;}
.iconLook{background: url(../images/my/icon-gr-kg.png) no-repeat;background-size: 100% 100%;}
.iocnLine{background-position:-177px -103px;}
.iconHotLine{background: url(../images/my/icon-gr-zg.png) no-repeat;background-size: 100% 100%;}
.iconIdea{background-position:-23px -103px;}
.order li .chak{float: right;font-size: 12px;color:#a2a2a2 ;padding-right: 5px;margin-top: 2px;}

.icon-return{position: absolute;top:50%;right: 10px; -webkit-transform: translate(0,-50%);transform: translate(0,-50%);}
.order li.orderList a >*{display: block;}
.order li.orderList{padding: 0;display: -moz-box; display: -ms-flexbox;display: -webkit-box; display: -webkit-flex; display: box; display: flexbox; display: flex;padding: 10px 0;    text-align: center;}
.order li.orderList a{ float:left; width: 25%; display:block;font-size: 12px;color:#a2a2a2 ;-webkit-box-flex:1; -moz-box-flex:1; -webkit-flex:1;-ms-flex:1;flex:1; position: relative;}
.order .iconPayment,.order .iocnGoods,.order .iconEvaluation,.order .iconReturn{position:relative;width: 26px;height: 26px; background: url(../images/ss-sprites.png) no-repeat; background-size: 225px 225px;margin: 0 auto;background-position:-74px -103px;}
.order .iocnGoods{background-position:-100px -103px;}
.order .iconEvaluation{background-position:-100px -130px;}
.order .iconReturn{background-position:-2px -130px;}
.orderPadding{padding:0 0 0 10px; background: #fff;border-bottom: 1px solid #e5e5e5;border-top: 1px solid #e5e5e5;}
.orderPadding .order li{ padding:12px 15px 12px 30px;}

.userHeader img {
    width: 22%;
    position: absolute;
    top: 26%;
    left: 50%;
    ransform: translate(-50%,-50%);
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    border-radius: 50%;
    overflow: hidden;
    border: 4px solid rgba(0, 0, 0, 0.21);
}

.orderPadding .order{ margin-bottom: 0;}

.orderPadding .order a:last-child li:last-child{border-bottom: none;}
.order li .chak em{color: #f23b3b;margin-left: 6px;}

.out{
	font-size:14px;
    text-align: center;
    padding: 12px 0px;
}

.outBox{
	width: 100%;  margin-bottom: 15px;
}

.outDiv{
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;
 }
 
.orderhint{
	box-sizing:border-box; -moz-box-sizing:border-box;-webkit-box-sizing:border-box; 
	border:thin solid red;
	color:#fff;
	font-size: 10px;
    line-height: 10px;
    width: 13px;
    height: 13px;
    text-align: center;
    background-color:red;
    position: absolute;
    top: -2px;
    left: 18px;
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    border-radius: 50%;
    letter-spacing: 0px;
    display: none;
} 
.personalBox{box-sizing:border-box;-webkit-box-sizing:border-box;float: left; width: 100%; padding: 0px 10px; background-color: #fff; margin-top: 10px; overflow: hidden;}
.personalButton{width: 100%; font-size: 16px; line-height: 18px; padding: 16px 0px;text-align: center; margin-top: 10px; float: left; background-color: #fff;}
.personalDiv{box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;	float: left; width: 100%; border-bottom: thin solid #eeeeee; height: 50px;position: relative;}
.personl-Modify{color: #666;position: absolute;top: 50%;right: 20px;-webkit-transform: translate(0,-50%);transform: translate(0,-50%);}
.personalHead{width: 30px; height: 30px;border-radius:50px;position: absolute;top: 50%;right: 20px;-webkit-transform: translate(0,-50%);transform:translate(0,-50%);overflow: hidden;}
.headImgf{filter: alpha(opacity=0);-moz-opacity: 0;opacity: 0;width: 100%; height: 42px; position: absolute; top:0px; right: 20px; background-color: black; z-index: 800;}
.personalBox .icon-return{right: 0;}
.personalDiv:last-child{border-bottom: 0;}

.ss-personBox{padding: 10px;font-size: 16px;}
.ss-itemsInput{background: #fff;margin: 15px auto 10px; border-radius: 6px;height: 42px;line-height: 42px;padding: 0 10px; position: relative;overflow: hidden;}
.ss-input-txt{font-size: 16px;padding: 0 10px;}
.ss-label{color: #cdcccd;}
.valid{color: #767676;font-size: 12px;}
.ss-btn-save{width: 100%;height: 42px;line-height: 42px;font-size: 16px;color: #fff;text-align: center;margin-top: 40px;}


.personalDiv{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;float: left; width: 100%; height: 42px; position: relative; overflow: hidden;background-color: #fff;line-height: 42px;
}
.personalDivP1{
	padding: 0 0 0 0; margin: 0 0 0 0;padding-left: 15px; float: left; font-family: 微软雅黑; font-size: 14px; line-height: 18px; margin-top: 12px;
}
.select-box{text-align: right;}
.select-boxAlert{position: absolute;bottom: 0;left: 0;width: 100%; color: #1ac9ba;z-index: 10000;background: #fff;display: none;text-align: center;}
.select-boxAlert span{display: block;line-height: 40px;border-bottom: thin solid #eeeeee;}
.select-boxAlert div{border-top: 10px solid #eee;line-height: 40px;}
