body{background: #eee;}
.addressList{margin-top: 10px;background: #fff;padding: 14px 18px;}
.addressList .name{color: #323232;font-size: 16px;overflow: hidden;margin-bottom: 4px;}
.addressList .name > *{float: left;}
.addressList .name .tel{float: right;}
.addressList .addressTxt{color: #8d8d8d;margin: 0;border-bottom: 1px solid #eeeeee;padding-bottom: 8px;}
.setting{padding-top: 10px;overflow: hidden;}
.setting .default{float: left;}
.setting .default i{display: inline-block;width: 12px;height: 12px;background:url(../images/ss-sprites.png) no-repeat;background-size:180px 180px;background-position: -132px -38px;margin-right: 5px;}
.setting .default i.cur{background-position: -121px -38px;}
.setting .default span{color: #323232;}
.setting .default span.cur{color: #ee4910;}
.setting .operate{float: right;}
.setting .operate .modify,.setting .operate .remove{background:url(../images/ss-sprites.png) no-repeat;background-size:250px 250px;width: 15px;height: 17px;display: inline-block;position: relative;top: 3px; right: 4px;background-position: -199px -53px;}
.setting .operate .remove{background-position: -183px -37px;}
.setting .operate span{margin-right: 24px;}
.setting .operate span:last-child{margin-right: 0;}
.addressForm{padding: 0 14px;}
.addressForm.new{padding: 0;}
.addressInput{margin-top: 15px;background: #fff;border-radius: 5px;padding: 0 16px;height: 42px; line-height: 42px; font-size: 14px;color: #8d8d8d;position: relative;}
.addressInput.new:first-child{margin-top: 15px;}
.addressInput.new{border-radius: 0;margin-top: 0;border-bottom: 1px solid #eeeeee;}
.addressInput input::-webkit-input-placeholder{color:#aeaeae;}
.addressInput input:-moz-placeholder{color:#aeaeae;}
.addressInput .label input::-webkit-input-placeholder{color:#666;}
.addressInput .label input:-moz-placeholder{color:#666;}
.addressForm .submit{background: #ee4910;color: #fff;font-size: 16px;width: 100%;line-height: 42px;border-radius: 5px;margin-top: 30px;}
.label input{ width: 72px;}
.addressInput i{width: 8px;height: 14px;position: absolute;right: 12px;top: 50%;-webkit-transform: translate(0,-50%);transform: translate(0,-50%);}
.addressInput.new input{width: 100%;height: 100%;}
.addressForm .submit.new{margin-top: 60px;}
.ss-btn-added{width: 100%;height: 40px;font-size: 16px;color: #fff;background-color: #ee4910;text-align: center;position: fixed;bottom: 0;max-width: 768px;margin: 0 auto;}

