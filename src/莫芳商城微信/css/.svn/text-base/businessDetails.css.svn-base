.aO_title{
	 width:100%; height:50px;background-color:#f7f7f7; float:left; border-bottom-color:#eeeeee; border-bottom-style:solid; border-bottom-width:thin;z-index: 999; position: relative;
}

.aO_titleI{
	width:22px; height:22px; margin-top:14px; float:left; margin-left:10px;
}

.aO_titleDD{
	transform: translate(-50%,0px);
	-ms-transform: translate(-50%,0px);		/* IE 9 */
	-webkit-transform: translate(-50%,0px);	/* Safari and Chrome */
	-o-transform: translate(-50%,0px);		/* Opera */
	-moz-transform: translate(-50%,0px);
	position:absolute; top:16px; left:50%;
	font-size:16px;  float:left; font-weight:bold; letter-spacing:2px; color:#323232;
}
.home img{
	width:25px; height:25px; float:right; margin:12px 20px 0 0; cursor:pointer;
}
