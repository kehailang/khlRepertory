body{
	-webkit-touch-callout:none;
	-webkit-user-select:none;
	-khtml-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select:none;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}

.waitAc_Back{
	position: fixed; z-index: 998; width: 100%; height: 100%; display: none;
	 filter:alpha(opacity=50);    
     -moz-opacity: 0.5;    
     opacity: 0.5; 
}

.waitAc_Data{
	transform:translate(-50%,-50%);
	-ms-transform:translate(-50%,-50%); 	
	-moz-transform:translate(-50%,-50%); 	
	-webkit-transform:translate(-50%,-50%); 
	-o-transform:translate(-50%,-50%); 
	-moz-border-radius: 10px; -webkit-border-radius: 10px; border-radius:10px; border:thin solid #eeeeee;
	position: fixed; z-index: 999; background-color: #fff; width: 80px; height: 120px; top:50%; left: 50%; display:none;
	box-shadow: 2px 2px 1px #777; 
}

.waitAc_Data img{
	width: 100%; float: left; display: block; border-radius:10px;
}
.waitAc_Data div{
	width: 100%; border-top: thin solid #eeeeee; float: left;
}

.waitAc_Data div p{
	width: 100%; float: left; padding: 0 0 0 0; margin: 0 0 0 0; margin-top: 10px; text-align: center; font-size: 12px; line-height: 14px;color: #7D7A7A;
}