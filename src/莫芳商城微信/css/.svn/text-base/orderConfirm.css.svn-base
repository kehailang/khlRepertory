.wrap{margin:10px auto 55px; float: left; width: 100%;}
.myOrder{margin: 20px 0; background: #fff;}
.myOrder .size{line-height: 35px;color: #8d8d8d;border-bottom: 1px solid #eeeeee;padding: 0 20px;}
.myOrder .size em{font-style: normal;float: right;color: #ee4910;}
.myOrder .product{padding: 20px 15px;overflow: hidden;position: relative;}
.orderList{border-bottom: 1px solid #eeeeee;}
.product .orderImg{float: left; width: 90px;height: 84px;border-radius: 5px;border: 1px solid #e5e5e5; overflow: hidden;margin-right: 10px;}
.product .orderImg img{width: 100%;height: 100%;}
.product .orderTxt{overflow: hidden;height: 90px;position: relative;}
.product .orderTxt h3{margin-top: 0;line-height: 16px;font-size: 15px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.product .orderTxt p{color: #8d8d8d;}
.product .orderTxt p span{margin-right: 12px;}
.product .orderTxt .sum{position: absolute;bottom: 0;left: 0;color: #ee4910;font-size: 14px;}
.product .orderTxt .sum em{font-size: 17px;color: #000;margin-left: 6px;}

.waitPayment{width: 100%;color: #fff;position: relative;text-align: center;}
.waitPayment img{display: block;}
.waitPayment > div{position: absolute;top: 50%;left: 50%;-webkit-transform: translate(-50%,-50%);-ms-transform: translate(-50%,-50%);transform: translate(-50%,-50%);width: 100%;height: 72px;}
.waitPayment i.waitIcon,.waitPayment i.succeedIcon,.waitPayment i.succeedPayIcon{background: url(../order/images/wait.png) no-repeat;width:56px;height: 57px;display: inline-block;background-size: 100%;margin-right: 15px;}
.waitPayment i.succeedIcon{background: url(../images/succed.png) no-repeat;background-size: 100%;}
.waitPayment i.succeedPayIcon{background: url(../images/succeed_pay.png) no-repeat;background-size: 100%;}
.waitPayment .wait{display: inline-block;position: relative; top: -5px;}
.waitPayment .wait h2{font-size: 34px;}
.waitPayment .wait h6{text-align: left;font-size: 13px;}
.myOrderEnvelope{position: relative;    margin: 2% 0;}
.envelopeCenter{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.myOrderEnvelope .name{color: #323232;font-size: 16px; }
.myOrderEnvelope .default{color: #8d8d8d;margin-top: 4%;line-height: 15px;position: relative;word-break: break-all;}
.envelopeCenterBox{position: absolute;top:50%;left:0;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);padding:0 8% 0 14%; width: 100%;box-sizing: border-box;}
.myOrderEnvelopePhone{float: right;margin-right: 10px;}


.envelopeCenterT{position: absolute;top: 50%;left: 2%;font-size:12px; line-height:14px; padding: 2px 4px; background:#ee4910 ;color: #fff;text-align: center;border-radius: 2px;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}

.addressIcon{display: inline-block;width: 6px;height: 10px;background: url(../images/public/return_icon.png);background-size: 100%; position: absolute;top:50%;right:14px;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}
.commodity{background: #fff;padding:12px 15px;overflow: hidden;position: relative;margin:0 0 2%;}
.commodity li{float: left;}
.commodity li.one{width: 30%;position: relative;border-right: 10px solid #fff;box-sizing: border-box;}
.commodity li.one img{width: 100%;height:100%;display: block;}
.commodity li.one span{position: absolute;bottom: 0;right: 0;width: 21px;height: 16px;line-height: 14px; display: inline-block;background: #cacaca;color: #fff;text-align: center;border-top-left-radius: 4px;border-bottom-right-radius: 4px;}
.commodity li.last img{width: 27px;height: 6px;}
.commodity li.one div{border: 1px solid #cacaca;border-radius: 4px;overflow: hidden;}
.commodity li.last{width: 20%;  float: right; padding-top: 10%;}
.commodity li.last span{color: #ee4910;display: block;text-align: right;}
.orderInformation{background: #fff;padding: 14px 15px;position: relative;}
.orderInformation p{color: #8d8d8d;line-height: 18px;margin: 0;}
.orderInformation button{position: absolute;top:14px;right: 14px; background: #ee4910;color: #fff;border-radius: 4px;}
.orderBtn{width: 100%;position: fixed;bottom: 0;left: 0;} 
.orderBtn .orderBtnBox{max-width: 750px;margin: 0 auto;}
.orderBtn a{display: inline-block;float: left;width: 50%;}

.fixed{position: fixed;bottom: 0;left: 0;width: 100%;z-index: 100;}
.onceAccount{background: #fff;border-top: 1px solid #e2e2e2;max-width: 750px;margin: 0 auto;overflow: hidden;}
.onceAccount > div{float: left;}
.onceAccount span,.onceAccount.threeBox span{overflow: hidden;width: 50%; height: 45px;line-height: 45px;display: inline-block;float: left;font-size: 13px;text-align: center;}
.onceAccount .right{background:#ee4910 ;color: #fff; width: 30%;}
.onceAccount .left{ width: 70%;}
.onceAccount span i{display: inline-block;width: 18px;height: 18px;position: relative;top: 5px;margin-right: 4px;}
.onceAccount .right i{background: url(../images/myOrder/vButton.png) no-repeat;background-size: 100%;}
.onceAccount.threeBox span i{top:3px;}
.onceAccount .left i{background: url(../images/order/cancel.png) no-repeat;background-size: 100%;}
.onceAccount.threeBox span{width: 33.33%;}
.onceAccount.threeBox i{width: 16px;height: 16px;}
.onceAccount.threeBox .left i{background: url(../images/sales_return.png) no-repeat;background-size: 100%;}
.onceAccount.threeBox .center i{background: url(../images/logistics.png) no-repeat;background-size: 100%;}
.onceAccount.threeBox .center{border-left: 1px solid #e2e2e2;box-sizing: border-box;}
.onceAccount.threeBox .right i{background: url(../images/evaluate.png) no-repeat;background-size: 100%;}

.waitPaymentBtn{background: #fff;overflow: hidden;border-bottom: 1px solid #e2e2e2;}
.waitPaymentBtn span{display: inline-block;width: 50%;float: left;text-align: center;font-size: 16px;padding: 10px 0;}
.waitPaymentBtn span:first-child a{border-left: none;}
.waitPaymentBtn span a{border-left: 1px solid #e2e2e2; width: 100%;display: inline-block;}

.myOrder{background: #fff;margin: 10px 0 45px; float: left; width: 100%;}
.orderList{padding: 20px 0;border-bottom: 1px solid #eeeeee;}
.myOrder .product{padding: 0 15px 0 15px;overflow: hidden;position: relative;}
.product .orderImg{float: left; width: 93px;height: 86px;border-radius: 5px;border: 1px solid #e5e5e5; overflow: hidden;margin-right: 10px;}
.product .orderImg img{width: 100%;}
.product .orderTxt{overflow: hidden; position: relative;height: 88px;}
.product .orderTxt h3{margin-top: 0;line-height: 16px;font-size: 15px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap; margin-bottom: 0px;}
.product .orderTxt p{color: #8d8d8d;}
.product .orderTxt p span{margin-right: 12px;}
.product .orderTxt p span:last-child{
	font-size:12px;
	margin-right: 0;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical; overflow:hidden;
	text-overflow: ellipsis;/* IE/Safari */-ms-text-overflow: ellipsis;-o-text-overflow: ellipsis;  word-wrap: break-word; -moz-binding: url("ellipsis.xml#ellipsis");
}
.product .coin{position: absolute;left: 0;bottom: -2px;color: #ee4910;font-size: 14px;}
.product .coin i{font-style: normal;}
.amount{
	-moz-border-radius: 5px;      /* Gecko browsers */
    -webkit-border-radius: 5px;   /* Webkit browsers */
    border-radius:5px;  
	position: absolute; bottom:0;right: 0;overflow: hidden;line-height: 16px; font-size: 14px; padding: 5px 10px; background-color: #ee4910; color: #fff;
}

.addressBox{
	float: left; width: 100%;
}

.addressDiv{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	float: left; width: 100%; padding: 10px 10px; background-color: #fff;  color:black;
}

.addressPs{
	width: 100%; float: left;
}

.addressNames{
	float: left; font-size: 14px; line-height: 16px;
}

.addressPhones{
	float: right; font-size: 14px; line-height: 16px;
}

.addressPs2{
	width: 100%; float: left; position: relative; margin-top: 10px; margin-bottom: 5px;
}

.addressDs{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	float: left; font-size: 12px; line-height: 14px; padding-left: 45px; margin-top: 2px;
}

.addressDFLogos{
	position:absolute; top:0; left:0;
	-moz-border-radius: 2px;      /* Gecko browsers */
    -webkit-border-radius: 2px;   /* Webkit browsers */
    border-radius:2px;  
	padding: 2px 5px; background-color: #ee4910; font-size: 12px; line-height: 14px; color: #fff; 
}

.addressDivActive{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	float: left; width: 100%; padding: 10px 10px; background-color: #7183AB;  color:#fff; 
}

.addressAddB{
	width: 20px; height: 20px; float: right; display: block; margin-top: 16px; margin-right: 14px;
}

.addressForm{
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
	    padding: 0 5%; float: left; margin-top: 10px; width: 100%;padding-bottom: 40px;
}
.addressForm.new{padding: 0;}
.addressInput{margin-top: 15px;background: #fff;border-radius: 5px;height: 42px;
    line-height: 43px;padding: 0 16px;font-size: 14px;color: #8d8d8d;position: relative;}
.addressInput:first-child{margin-top:0;}    
.addressInput.new:first-child{margin-top: 15px;}
.addressInput.new{border-radius: 0;margin-top: 0;border-bottom: 1px solid #eeeeee;}
.addressInput input::-webkit-input-placeholder{color:#8d8d8d;line-height: 18px;}
.addressInput input:-moz-placeholder{color:#8d8d8d;line-height: 18px;}
.addressForm .submit{background: #ee4910;color: #fff;font-size: 16px;width: 100%;line-height: 35px;border-radius: 5px;margin-top: 30px;}
.addressInput input{width: 70%; top: 0;-webkit-tap-highlight-color: rgba(255,255,255,0);-webkit-appearance: none;outline:none;  margin-top: 22px;position: absolute; transform: translate(0,-50%); -webkit-transform: translate(0,-50%); -ms-transform: translate(0,-50%);font-size: 14px; -webkit-appearance: none;}
.addressInput i{display: inline-block;width: 6px;height: 10px;background:url(../images/address/addressTriangle.png) no-repeat;background-size: 100%;position: absolute;right: 12px;top: 16px;}
.addressInput.new input{width: 100%;height: 100%;}
.addressForm .submit.new{margin-top: 60px;}
.addressForm .submit:ACTIVE{background:#C74112;}
