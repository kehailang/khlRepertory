body{
	-webkit-touch-callout:none;
	-webkit-user-select:none;
	-khtml-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select:none;
}

.aO_title{
	 width:100%; height:50px;background-color:#ffffff; float:left; border-bottom-color:#eeeeee; border-bottom-style:solid; border-bottom-width:thin;
}

.aO_titleI{
	width:22px; height:22px; margin-top:14px; float:left; margin-left:10px;
}

.aO_titleD{
	width:39px;
}

.aO_titleDD{
	font-size:16px; margin-top:16px; float:left; font-weight:bold; letter-spacing:2px; color:#323232;
}

.aO_div{
	width:100%; float:left; height:auto; position:relative; margin-top: 20px;

}

.aO_divD1{
	width:100%; float:left; margin-top:10px; position:relative; height:42px;text-align:center;
}
.aO_divD1ImgBox{
	width:40px; height:42px; float:left; background-color:#FFFFFF; margin-left:5%;
	/* Gecko browsers */
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 0;
	-moz-border-radius-bottomleft: 5px;
	-moz-border-radius-bottomright: 0;
 
	/* Webkit browsers */
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 0;
	-webkit-border-bottom-left-radius: 5px;
	-webkit-border-bottom-right-radius: 0;
 
	/* W3C syntax */
	border-top-left-radius: 5px;
	border-top-right-radius: 0;
	border-bottom-left-radius:  5px;
	border-bottom-right-radius: 0;
}

.aO_divD1Img{
	height:22px; float:right; margin-top:10px;
}


.aO_divD1btnBox{
	width:40%;  height:42px; border:none; float:left; background-color:#FFFFFF; position:relative;
	/* Gecko browsers */
	-moz-border-radius-topleft: 0px;
	-moz-border-radius-topright: 5px;
	-moz-border-radius-bottomleft: 0px;
	-moz-border-radius-bottomright: 5px;
 
	/* Webkit browsers */
	-webkit-border-top-left-radius: 0;
	-webkit-border-top-right-radius: 5px;
	-webkit-border-bottom-left-radius: 0;
	-webkit-border-bottom-right-radius: 5px;
 
	/* W3C syntax */
	border-top-left-radius: 0;
	border-top-right-radius: 5px;
	border-bottom-left-radius:  0;
	border-bottom-right-radius: 5px;

}

.aO_divD1btn{
	-webkit-tap-highlight-color: rgba(255,255,255,0);
	-webkit-appearance: none;
	outline:none;
	margin-left:10px;
	width:80%; outline:none; height:15px; font-size:13px; font-weight:bold; letter-spacing:0px; line-height:15px; float:left; margin-top:14px; 
	border-style: solid;
	border-width: thin;
	border-color:#ffffff;
	padding-bottom:0px;
	padding-top:0px;	
}


.aO_divD2{
	width:100%; float:left; margin-top:10px; position:relative; height:42px;text-align:center;
}
.aO_divD2ImgBox{
	width:40px; height:42px; float:left; background-color:#FFFFFF; margin-left:5%;
	/* Gecko browsers */
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 0;
	-moz-border-radius-bottomleft: 5px;
	-moz-border-radius-bottomright: 0;
 
	/* Webkit browsers */
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 0;
	-webkit-border-bottom-left-radius: 5px;
	-webkit-border-bottom-right-radius: 0;
 
	/* W3C syntax */
	border-top-left-radius: 5px;
	border-top-right-radius: 0;
	border-bottom-left-radius:  5px;
	border-bottom-right-radius: 0;
}

.aO_divD2Img{
	height:22px; float:right; margin-top:10px;
}


.aO_divD2btnBox{
	width:40%;  height:42px; border:none; float:left; background-color:#FFFFFF; position:relative;
}

.aO_divD2btn{
	-webkit-tap-highlight-color: rgba(255,255,255,0);
	-webkit-appearance: none;
	outline:none;
	margin-left:10px;
	width:80%; outline:none; height:15px; font-size:13px; font-weight:bold; letter-spacing:0px; line-height:15px; float:left; margin-top:14px; 
	border-style: solid;
	border-width: thin;
	border-color:#ffffff;
	padding-bottom:0px;
	padding-top:0px;	
}

.aO_divD2text{
	width:100px;  background-color:#ee4910; color:#FFFFFF; font-size:12px;float:left; letter-spacing:-1px; text-align:center; line-height:20px; padding-top:11px; padding-bottom:11px;

	/* Gecko browsers */
	-moz-border-radius-topleft: 0px;
	-moz-border-radius-topright: 5px;
	-moz-border-radius-bottomleft: 0px;
	-moz-border-radius-bottomright: 5px;
 
	/* Webkit browsers */
	-webkit-border-top-left-radius: 0;
	-webkit-border-top-right-radius: 5px;
	-webkit-border-bottom-left-radius: 0;
	-webkit-border-bottom-right-radius: 5px;
 
	/* W3C syntax */
	border-top-left-radius: 0;
	border-top-right-radius: 5px;
	border-bottom-left-radius:  0;
	border-bottom-right-radius: 5px;
}

.aO_divD2text:active{
	background-color:#CC4010;
}


.aO_divD3btn{
	-webkit-tap-highlight-color: rgba(255,255,255,0);
	-webkit-appearance: none;
	float:left; margin-left:5%; margin-top:30px; text-align:center;
	width:90%; color:#FFFFFF; background-color:#ee4910; height:atuo; border:none; font-size:15px; font-weight:bold; letter-spacing:2px; vertical-align:middle;
	padding-top:10px; padding-bottom:10px; line-height:22px;float:left;
	border-radius: 5px; /* 所有角都使用半径为5px的圆角，此属性为CSS3标准属性 */
	-moz-border-radius: 5px; /* Mozilla浏览器的私有属性 */
	-webkit-border-radius: 5px; /* Webkit浏览器的私有属性 */ 
}

.aO_divD3btn:active{
	background-color:#CC4010;
}

.aO_divD4{
	width:90%; margin-top:5px; font-size:12px; color:#bebebe; margin-left:5%; float:left;
}

.aO_divD4radio{
	float:left; line-height:20px;
}

.aO_divD4text1{
