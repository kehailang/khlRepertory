.alert-text{word-break:break-all;float: left;  text-align: center; font-size: 14px; color: black; width: 100%;}
.alert-button{-webkit-transform:translate(-50%,0);transform:translate(-50%,0); border-radius:5px;
	float: left; width: 100px; color: #fff; font-size: 14px;background-color: #ff3f26; margin-top: 15px; margin-left:50%;line-height: 16px; padding-bottom: 5px; padding-top: 5px; text-align: center;}
