#page1{ float: left; width: 100%; height: 100%;}
#page2{ float:left; width: 100%; display: none; height: 100%;}

.myOrderEnvelope{position: relative;    margin: 2% 0;}
.envelopeCenter{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.envelopeCenterT{position: absolute;top: 50%;left: 2%;font-size:12px; line-height:14px; padding: 2px 4px; background:#ee4910 ;
color: #fff;text-align: center;border-radius: 2px;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}
.myOrderEnvelope .name{color: #323232;font-size: 16px; }
.myOrderEnvelope .default{color: #8d8d8d;margin-top: 4%;line-height: 15px;position: relative;word-break: break-all;}
.envelopeCenterBox{position: absolute;top:50%;left:0;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);padding:0 8% 0 14%; width: 100%;box-sizing: border-box;}
.myOrderEnvelopePhone{float: right;margin-right: 10px;}

.addressIcon{display: inline-block;width: 6px;height: 10px;background: url(../images/public/return_icon.png);background-size: 100%; position: absolute;top:50%;right:14px;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}
.commodity{background: #fff;padding:12px 15px;overflow: hidden;position: relative;margin:0 0 2%;}
.commodity li{float: left;}
.commodity li.one{width: 30%;position: relative;border-right: 10px solid #fff;box-sizing: border-box;}
.commodity li.one img{width: 100%;height:100%;display: block;}
.commodity li.one span{position: absolute;bottom: 0;right: 0;width: 21px;height: 16px;line-height: 14px; display: inline-block;background: #cacaca;color: #fff;text-align: center;border-top-left-radius: 4px;border-bottom-right-radius: 4px;}
.commodity li.last img{width: 27px;height: 6px;}
.commodity li.one div{border: 1px solid #cacaca;border-radius: 4px;overflow: hidden;}
.commodity li.last{width: 20%;  float: right; padding-top: 10%;}
.commodity li.last span{color: #ee4910;display: block;text-align: right;}
.orderInformation{background: #fff;padding: 14px 15px;position: relative;}
.orderInformation p{color: #8d8d8d;line-height: 18px;margin: 0;}
.orderInformation button{position: absolute;top:14px;right: 14px; background: #ee4910;color: #fff;border-radius: 4px;}
.orderBtn{width: 100%;position: fixed;bottom: 0;left: 0;} 
.orderBtn .orderBtnBox{max-width: 750px;margin: 0 auto;}
.orderBtn a{display: inline-block;float: left;width: 50%;}

.confirm{position: fixed;bottom: 0;left: 0;width: 100%;z-index: 100;}
.gopay{background: #fff;border-top: 1px solid #e2e2e2;max-width: 750px;margin: 0 auto;overflow: hidden;}
.gopay > div{float: left;}
.gopay span,.onceAccount.threeBox span{overflow: hidden;width: 50%; height: 45px;line-height: 45px;display: inline-block;float: left;font-size: 13px;text-align: center;}
.gopay .right{background:#ee4910 ;color: #fff; width: 30%;}
.gopay .left{ width: 70%;}
.gopay span i{display: inline-block;width: 18px;height: 18px;position: relative;top: 5px;margin-right: 4px;}
.gopay .right i{background: url(../images/myOrder/vButton.png) no-repeat;background-size: 100%;}
.gopay.threeBox span i{top:3px;}
.gopay .left i{background: url(../images/myOrder/cancel.png) no-repeat;background-size: 100%;}
.gopay.threeBox span{width: 33.33%;}
.gopay.threeBox i{width: 16px;height: 16px;}
.gopay.threeBox .left i{background: url(../images/myOrder/sales_return.png) no-repeat;background-size: 100%;}
.gopay.threeBox .center i{background: url(../images/myOrder/logistics.png) no-repeat;background-size: 100%;}
.gopay.threeBox .center{border-left: 1px solid #e2e2e2;box-sizing: border-box;}
.gopay.threeBox .right i{background: url(../images/myOrder/evaluate.png) no-repeat;background-size: 100%;}

