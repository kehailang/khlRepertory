body{background: #eee;}
.myOrder,.myOrder2{background: #fff;margin: 10px 0 45px;}
.orderList,.orderList2{padding: 20px 0;border-bottom: 1px solid #eeeeee;}
.myOrder .product,.myOrder2 .product{padding:0 10px 0 35px;overflow: hidden;position: relative;}
.product .orderImg{float: left; width: 88px;height: 82px;border-radius: 5px;border: 1px solid #e5e5e5; overflow: hidden;margin-right: 10px;}
.product .orderImg img{width: 100%;}
.product .orderTxt{overflow: hidden; position: relative;height: 88px;}
.product .orderTxt h3{margin-top: 0;line-height: 16px;font-size: 15px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.product .orderTxt p{color: #8d8d8d;margin-top: 8px;font-size: 12px;}
.product .orderTxt p span:last-child{margin-right: 0;}
.product .coin{position: absolute;left: 0;bottom: -2px;color: #ee4910;font-size: 14px;}
.product .coin i{font-style: normal;}
.amount{position: absolute; bottom:0;right: 0;width: 90px;border: 1px solid #e2e2e2;border-radius:5px;overflow: hidden;line-height: 21px;height: 21px; }
.amount >*{float: left;width: 33.33%;text-align: center;}
.amount span i{display: inline-block;position: relative;width: 11px;}
.amount .min i{background: url(../images/myOrder/min.jpg) no-repeat;background-size: 100%;height: 2px;top: -4px;left:0;}
.amount .add i{background: url(../images/myOrder/add.jpg) no-repeat;background-size: 100%;height: 11px;top:0px;left: 0;}
.amount .text_box{width: 100%;text-align: center; border-left: 1px solid #e2e2e2;border-right: 1px solid #e2e2e2;height: 21px;box-sizing: border-box;-webkit-appearance: none;border-radius: 0;}
.product > span,.onceAccount .left span{width: 20px;height: 20px;display: inline-block;background:url(../images/ss-sprites.png) no-repeat;background-size:225px 225px;background-position:-207px -72px;position: absolute;top: 50%;left: 10px;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}
.product > span.cur,.onceAccount .left span.cur{background-position:-187px -72px;}
.fixed{position: fixed;bottom: 0;left: 0;width: 100%;z-index: 100;}
.onceAccount{background: #fff;border-top: 1px solid #e2e2e2;max-width: 750px;margin: 0 auto;overflow: hidden;}
.onceAccount > div{float: left;}
.myOrder2 .amount{ border: none; color: #ee4910; bottom: 34px; right: -40px;}

.onceAccount .left{overflow: hidden;height: 45px;line-height: 24px;padding-left: 35px;box-sizing: border-box; position: relative;  width: 100%; box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box; padding-right: 125px; }
.onceAccount .right{width: 125px; height: 46px;line-height: 45px;background: #ee4910;font-size: 15px;color: #fff;text-align: center;float: right;position:absolute; top:0px; right: 0px;}

.onceAccount .left >*{    display: inline-block;}
.onceAccount .left p{position: absolute;margin: 0;right: 140px;top:50%;color: #8d8d8d;-webkit-transform: translate(0,-50%);-ms-transform: translate(0,-50%);transform: translate(0,-50%);}
.onceAccount .left p em,.onceAccount .right i{font-style: normal;}
.onceAccount .left > div{height: 46px;line-height: 46px;}
.onceAccount .right i img{position: absolute;top:15px;left:22px;width: 16px;height: 16px; }
.onceAccount .right i{padding-left: 18px;}

